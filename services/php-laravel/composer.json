{
  "name": "discord-webhook/php-laravel",
  "description": "Discord webhook service using PHP/Laravel",
  "type": "project",
  "license": "MIT",
  "require": {
    "php": "^8.5",
    "laravel/framework": "^12.46",
    "google/cloud-pubsub": "^2.0"
  },
  "require-dev": {
    "laravel/pint": "^1.13"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "scripts": {
    "lint": "pint --test",
    "lint:fix": "pint"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "php-http/discovery": true
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
